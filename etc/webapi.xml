<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/adorncommerce-dropshipment/supplier" method="POST">
		<service class="Adorncommerce\Dropshipment\Api\SupplierRepositoryInterface" method="save"/>
